{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "lint": "eslint --ext .js --ext .jsx .",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test karma start --single-run",
    "test:watch": "NODE_ENV=test nodemon --exec karma start",
    "clean": "rimraf dist",
    "deploy": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "author": "Benjamin Kluck",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^6.1.2",
